{
  "name": "<%= appName %>",
  "productName": "<%= classifiedAppName %>",
  "version": "1.0.0",
  "description": "<%= appName %> by Soixante circuits",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= appName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "electronVersion": "0.35.0",
  "scripts": {
    "start": "export NODE_ENV='dev'; electron .",
    "build": "export NODE_ENV='production'; electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --all --version=$npm_package_electronVersion"
  },
  "files": [
    "index.js",
    "app/"
  ],
  "dependencies": {
    "crossroads": "^0.12.2",
    "electron-debug": "^0.5.0",
    "hasher": "^1.2.0",
    "signals": "^1.0.0",
    "vue": "^1.0.10"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.0",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.35.0",
    "html-loader": "^0.4.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9"
  }
}
